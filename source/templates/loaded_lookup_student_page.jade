extend default
			
block content
	.data-report-page
		.form
			form.data-report(action='/newpage', method = 'GET')
				//- p Please enter the inforation to generate the students.
				input(type='text', placeholder='Job Title')
				input(type='text', placeholder='Degree Level')
				input(type='text', placeholder='Term Graduated')
				input(type='text', placeholder='Year Graduated')
				input(type='text', placeholder='Location')
				input(type='text', placeholder='Skills')

				| full-time 
				input(type='checkbox', name='Full Time') 

				| internship
				input(type='checkbox', name='Internship')
				
				input(type='text', placeholder='GPA')
				input(type='text', placeholder='Salary')
				button(onclick="move(); type='submit', return false;") Homepage
